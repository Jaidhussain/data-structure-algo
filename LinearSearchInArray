#include<iostream>
using namespace std;
int main()
{
	int array[10],n,x,i;
	cout<<"enter the size of an array:";
	cin>>n;
	cout<<"enter array element:";
	for(int i=0; i<n; i++)
	{
		cin>>array[i];
	}
	cout<<"enter element in found at array element:";
	cin>>x;
	for( i=0; i<n; i++)
	{
		if(array[i]==x)
		{
			cout<<"element found at index:"<<i;
			break;
		}
		
	}
	if(i==n)
	{
		cout<<"element not found in array:";
	}
	
	return 0;
}

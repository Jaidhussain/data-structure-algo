#include<iostream>
using namespace std;
int main()
{
	int array[10],n,x,i;
	cout<<"enter the size of the array:";
	cin>>n;
	cout<<"enter array element:";
	for(i=0; i<n; i++)
	{
		cin>>array[i];
	}
	cout<<"enter the array element at the end:";
	cin>>x;
	array[i]=x;
	n++;
	for(int i=0; i<n; i++)
	{
		cout<<array[i]<<endl;
	}
	return 0;
}

#include<iostream>
using namespace std;
int main()
{
	int array[10],n,x;
	cout<<"enter the size of an array:";
	cin>>n;
	cout<<"enter the element:";
	for(int i=0; i<n; i++)
	{
		cin>>array[i];
	}
	cout<<"enter the new elements at the beggining:";
	cin>>x;
	for(int i=n; i>0; i--)
	{
		array[i]=array[i-1];
	}
	array[0]=x;
	n++;
	cout<"the elements are:";
	for(int i=0; i<n; i++)
	{
		cout<<array[i]<<endl;
	}
	return 0;
}
